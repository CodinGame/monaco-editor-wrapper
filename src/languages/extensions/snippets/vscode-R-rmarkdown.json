  "Insert unchecked item": {
    "prefix": "unchecked item",
    "body": "- [ ] ${1:${TM_SELECTED_TEXT}}",
    "description": "Insert unchecked item"
  },
  "Insert horizontal rule": {
    "prefix": "horizontal rule",
    "body": "----------\n",
    "description": "Insert horizontal rule"
