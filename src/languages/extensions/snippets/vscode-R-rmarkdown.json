  "Insert speaker notes": {
    "prefix": "speaker notes",
    "body": [
      "::: notes",
      "",
      "${TM_SELECTED_TEXT}$0",
      "",
      ":::"
    ],
    "description": "Insert speaker notes"
  },
  "Insert two columns": {
    "prefix": "two columns",
    "body": [
      ":::::: {.columns}",
      "::: {.column}",
