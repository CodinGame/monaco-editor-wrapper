        {
          "comment": "String",
          "match": "^\\s*(#[0-9]+) = (String)\\s+(#[0-9]+)\\s+(//)(.*)$",
          "captures": {
            "1": {
              "name": "constant.symbol.jvm-bytecode-verbose"
            },
            "2": {
              "name": "support.type.class.jvm-bytecode-verbose"
            },
            "3": {
              "name": "constant.symbol.jvm-bytecode-verbose"
            },
            "4": {
              "name": "comment.line.jvm-bytecode-verbose"
