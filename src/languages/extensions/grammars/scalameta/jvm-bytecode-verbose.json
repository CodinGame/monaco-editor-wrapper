          "end": "$",
          "beginCaptures": {
            "1": {
              "name": "constant.symbol.jvm-bytecode-verbose"
            },
            "2": {
              "name": "keyword.other.jvm-bytecode-verbose"
            },
            "3": {
              "name": "constant.symbol.jvm-bytecode-verbose"
            },
            "4": {
              "name": "keyword.other.jvm-bytecode-verbose"
            },
            "5": {
              "name": "constant.symbol.jvm-bytecode-verbose"
            },
            "6": {
