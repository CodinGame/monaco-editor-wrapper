    "@types/node": "14.14.31",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "babel-eslint": "10.1.0",
    "cson-parser": "4.0.9",
    "deep-equal": "2.0.5",
    "eslint": "8.9.0",
