  "browserslist": [
    "defaults",
    "chrome >= 59",
    "firefox >= 68",
    "safari >= 12",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "volta": {
    "node": "25.2.0",
    "npm": "11.6.2"
  },
  "release": {
    "extends": "@codingame/semantic-release-config-github"
  },
  "jest": {
    "resolver": "<rootDir>/jest/resolver.cjs",
    "testEnvironment": "./FixJSDOMEnvironment.js",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "^(..?/.+).js?$": "$1"
    },
    "setupFilesAfterEnv": [
      "fake-indexeddb/auto",
      "<rootDir>/browserMock.js"
    ],
    "transformIgnorePatterns": [
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "transform": {
      "^(?!.*\\.(js|ts|css|json)$)": "<rootDir>/jest/fileTransform.cjs",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.cjs",
      "^.+\\.(js|ts)$": [
        "<rootDir>/node_modules/babel-jest",
        {
          "configFile": "./babel.test.config.cjs"
        }
      ]
    }
  },
  "overrides": {
    "globals": "15.14.0",
    "monaco-editor": "$monaco-editor",
    "canvas": "$canvas"
  }
}
