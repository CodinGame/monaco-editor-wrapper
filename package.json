{
  "name": "@codingame/monaco-editor-wrapper",
  "version": "0.0.0-semantic-release",
  "private": false,
  "description": "Enhanced Monaco editor with TextMate grammars and more",
  "scripts": {
    "build": "npm run lint && rollup --config rollup.config.ts --configPlugin typescript && npm run generate-types",
    "generate-types": "tsc --project tsconfig.types.json",
    "update-extensions": "ts-node update-extensions.ts",
    "lint": "eslint --ext .ts src update-extensions.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodinGame/monaco-editor-wrapper"
  },
  "moduleResolution": "node",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "files": [
    "dist/",
    "stats.html"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "monaco-editor": "npm:@codingame/monaco-editor@^0.33.6",
    "vscode-oniguruma": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-optional-chaining": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@babel/runtime": "7.18.3",
    "@codingame/eslint-config": "^1.1.2",
    "@codingame/tsconfig": "^1.0.5",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/ini": "1.3.31",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "16.11.7",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "cson-parser": "4.0.9",
    "deep-equal": "2.0.5",
    "eslint": "8.17.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "fast-plist": "0.1.2",
    "ini": "3.0.0",
    "js-yaml": "^4.1.0",
    "json5": "2.2.1",
    "monaco-emacs": "^0.3.0",
    "monaco-vim": "^0.3.4",
    "postcss": "8.4.14",
    "rollup": "2.75.6",
    "rollup-plugin-json5": "1.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "5.6.0",
    "rollup-plugin-web-worker-loader": "1.6.1",
    "ts-node": "10.8.1",
    "tslib": "^2.4.0",
    "typescript": "4.7.3",
    "vscode-textmate": "^6.0.0"
  },
  "browserslist": [
    "defaults",
    "chrome >= 59",
    "firefox >= 68",
    "safari >= 12",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "volta": {
    "node": "16.14.0",
    "npm": "8.5.0"
  }
}
