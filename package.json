{
  "name": "@codingame/monaco-editor-wrapper",
  "version": "0.0.0-semantic-release",
  "private": false,
  "description": "Enhanced Monaco editor with TextMate grammars and more",
  "scripts": {
    "build": "npm run lint && npm run compile && npm run generate-types",
    "compile": "node --experimental-json-modules ./node_modules/.bin/rollup --config rollup.config.ts --configPlugin typescript",
    "generate-types": "tsc --project tsconfig.types.json",
    "update-extensions": "node --loader ts-node/esm scripts/update-extensions.ts",
    "download-extensions": "./scripts/download-extensions.sh",
    "lint": "eslint --ext .ts src scripts/update-extensions.ts",
    "preprepare": "npm run download-extensions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodinGame/monaco-editor-wrapper"
  },
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist/",
    "stats.html"
  ],
  "dependencies": {
    "@codingame/monaco-vscode-accessibility-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-audio-cue-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-configuration-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-debug-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-dialogs-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-editor-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-extensions-service-override": "1.83.0-next.0",
    "@codingame/monaco-vscode-keybindings-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-language-detection-worker-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-languages-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-lifecycle-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-markers-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-model-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-output-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-preferences-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-quickaccess-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-rollup-vsix-plugin": "^1.83.0-next.0",
    "@codingame/monaco-vscode-search-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-snippets-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-storage-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-textmate-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-theme-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-view-status-bar-service-override": "^1.83.0-next.0",
    "@codingame/monaco-vscode-views-service-override": "^1.83.0-next.0",
    "monaco-editor": "0.44.0",
    "vscode": "npm:@codingame/monaco-vscode-api@1.83.0-next.0"
  },
  "devDependencies": {
    "@codingame/commitlint-config-codingame": "^1.0.5",
    "@codingame/eslint-config": "^1.1.10",
    "@codingame/semantic-release-config": "^1.3.3",
    "@codingame/tsconfig": "^1.1.1",
    "@commitlint/cli": "^17.7.1",
    "@rollup/plugin-alias": "5.0.0",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/ini": "1.3.31",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "20.5.2",
    "@types/rollup-plugin-node-builtins": "^2.1.2",
    "@types/vscode": "~1.81.0",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "@vscode/vsce": "^2.20.1",
    "azure-devops-node-api": "^12.1.0",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "cson-parser": "4.0.9",
    "deep-equal": "2.2.2",
    "eslint": "8.47.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "fast-plist": "0.1.3",
    "ini": "4.1.1",
    "js-yaml": "^4.1.0",
    "json5": "2.2.3",
    "monaco-emacs": "^0.3.0",
    "monaco-vim": "^0.4.0",
    "postcss": "8.4.28",
    "rollup": "3.28.1",
    "rollup-plugin-glob-import": "^0.5.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-visualizer": "5.9.2",
    "ts-node": "10.9.1",
    "tslib": "^2.6.2",
    "typescript": "5.1.6"
  },
  "browserslist": [
    "defaults",
    "chrome >= 59",
    "firefox >= 68",
    "safari >= 12",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "volta": {
    "node": "18.13.0",
    "npm": "9.4.1"
  },
  "overrides": {
    "rollup": "3.28.1",
    "typescript": "5.1.6"
  }
}
