{
  "extends": "@codingame/tsconfig",
  "compilerOptions": {
    "baseUrl": "./app",
    "noImplicitOverride": true,
    "resolveJsonModule": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "noEmit": true,
    "paths": {
      "app/*": [
        "*"
      ]
    },
    "sourceMap": true,
    "target": "es2020"
  },
  "include": [
    "src",
    "scripts/update-extensions.ts",
    "rollup.config.ts",
    "jest.config.ts",
    "babel.test.config.cjs"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}